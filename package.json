{
  "name": "@liautaud/typezod",
  "version": "1.0.0",
  "description": "Zod-like DSL for comparing TypeScript compiler types against user-defined schemas",
  "type": "module",
  "private": true,
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.mts"
    }
  },
  "main": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsdown",
    "typecheck": "tsc --noEmit",
    "test": "vitest run"
  },
  "peerDependencies": {
    "typescript": ">=5.0.0"
  },
  "devDependencies": {
    "tsdown": "^0.11.9",
    "typescript": "^5.9.3",
    "vitest": "^3.2.4"
  }
}
